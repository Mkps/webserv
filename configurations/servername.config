server {
	listen 8080;
	autoindex on;
	host 127.0.0.1;
	root ./resources/;
    server_name example42.fr;
	
	allowedMethods GET|POST;
	location_ends py {
		cgi_path /usr/bin/python3;
	}

#	rien juste un commentaire

	location / {
		autoindex on;
		allowedMethods GET;
		index index.html;
	}
}

server {
	listen 8080;
	server_name example42.com;
	host 127.0.0.1;
	root ./static/;

	allowedMethods GET|POST;
	client_max_body_size 7M;

	error_page 404 resources/miaou.png;

#	rien juste un commentaire

	location / {
		autoindex on;
		allowedMethods GET;
		index index.html;
	}
}

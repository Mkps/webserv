server {
	listen 8080;
	autoindex on;
	host 0.0.0.0;
	root ./resources/;
	
	allowedMethods GET|POST;
	location_ends py {
		cgi_path /usr/bin/python3;
	}

#	rien juste un commentaire

	location / {
		autoindex on;
		allowedMethods GET;
		index index.html;
	}
}

